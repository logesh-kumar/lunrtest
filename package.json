{
  "name": "lunrtest",
  "private": true,
  "version": "0.0.0",  
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "debug": "npm run build && npm run watch-debug",
    "server:build-ts": "tsc -p ./server",
    "server:build": "npm run server:build-ts",
    "server:serve-debug": "nodemon --inspect server/dist/server.js",
    "server:serve": "node server/dist/server.js",
    "server:start": "npm run serve",
    "server:watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\"  \"npm run server:watch-ts\" \"npm run server:serve-debug\"",
    "server:watch-node": "nodemon server/dist/server.js",
    "server:watch-ts": "tsc -w server/**/*.ts",
    "server:watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run server:watch-ts\" \"npm run server:watch-node\"",
    "run-all": "concurrently \"npm run server:build-ts\" \"npm run server:watch\" \"npm run dev\""
  },
  "dependencies": {
    "@types/lodash": "^4.14.186",
    "@types/lunr": "^2.3.4",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.5.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "lunr": "^2.3.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "uuid": "^9.0.0",
    "@faker-js/faker": "^7.6.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
